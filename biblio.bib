@incollection{MoirIan2013DBN,
abstract = {Summary This chapter discusses digital data buses and communications networks used in current civil transport aircraft with a brief description of some common military data bus types. The purpose is to summarise the essential features and technologies of data buses and communications networks in common use in civil transport avionics systems so that the reader can appreciate the concepts involved, contrast their performance and attributes and hence understand what makes each best suited to a particular application. An explanation is given of basic principles of bus technologies including encoding techniques, attributes, linear, ring and star topologies followed by a discussion of how these principles have been applied to explore the features and evolution of significant data bus networks seen in today's civil transport aircraft. Descriptions are given of bus types such as ARINC 429 multi‐drop, MIL‐STD‐1553B command‐response protocol, ARINC 629 carrier sense — multiple access collision avoidance, ARINC 664 packet switched network (also sometimes known as aviation full‐duplex — AFDX network) and CANbus. Also discussed are other databus types such as Ethernet, Avionics Standard Communications Bus (ASCB), Commercial Standard Data Bus (CSDB), RS232, RS422 and IEEE 1394 Firewire 800 and the principles of fibre‐optic communications.},
pages = {79--118},
publisher = {John Wiley \& Sons, Ltd},
booktitle = {Civil Avionics Systems},
isbn = {9781118341803},
year = {2013},
title = {Chapter 3. Data Bus Networks},
language = {eng},
address = {Chichester, UK},
author = {Moir, Ian and Seabridge, Allan and Jukes, Malcolm},
keywords = {Data Bus ; Packet Switched Networks ; Communication Protocols ; Network Topology ; Virtual Links ; Arinc 429 ; Arinc 629 ; Arinc 664 ; Canbus ; Afdx ; Mil‐Std‐1553b ; Ieee 1394 ; Fibre‐Optics},
}

